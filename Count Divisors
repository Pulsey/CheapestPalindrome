import java.util.Scanner;
 
/**
 *
 * @author Gagan_000
 */
public class countdiv {
    public static void main(String[] args) {
        Scanner x=new Scanner(System.in);
        int n=x.nextInt();
        int m=x.nextInt();
        int k=x.nextInt();
        int count=0;
        
        for(int i=n;i<=m;i++)
        {
        if(i%k==0)
            count++;
        
        
        }
        System.out.println(count);
    }
}
Language: Java
